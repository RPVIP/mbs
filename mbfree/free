echo -e "${LETRAPRETA}            TESTADOR 10             ${NADA}"
echo -e "${AZULALL}====================================${NADA}"

TesterElegibility10=$(echo $impressionid10 | grep -n - | grep -c 1:)
case $TesterElegibility10 in
1) ;;
*)echo -e "${RED}NÃO ELEGIVEL NESSE TESTADOR${NADA}"; sleep 3; Ler11;;
esac


for i in $(seq 25);
do
reward10=$(curl -s "http://e.vivo.ddivulga.com/api/clickEvent" -d "advId=$advid10&eventImpressionId=$impressionid10&apass=$AccessPass")
rw=$(echo $reward10 | grep -c "evtClickId")
case $rw in
1) echo -e "${GREEN}PROVAVEL ENVIO DOS MBS - $i/25${NADA}" ; sleep 3;;
0)echo -e "${RED}NÃO ELEGIVEL NESSE TESTADOR${NADA}"; sleep 3; Ler11;;
esac
done
echo
echo
Ler11
}


Ler11(){ 
am start -n com.termux/com.termux.app.TermuxActivity
clear
Ler12
}
Ler12(){
NumeroDeTelefone=$(cat tokencreator2.txt | grep -o msisdn.................................... | cut -d '"' -f3 | grep -n 55 | grep 1: | cut -d':' -f2)
echo -e "1" >> TestesFeitos.txt
TestesFeitos=$(cat TestesFeitos.txt | wc -l)
echo -e "${VermelhoAll}            TESTAGEM FINALIZADA             ${NADA}"
echo -e "${NADA}============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"
echo -e "${CINZA}NUMERO DE TELEFONE USADO:${NADA} ${GREEN}$NumeroDeTelefone${NADA}"
echo -e "${CINZA}TESTES FEITOS COM ESSES TOKENS:${NADA} ${GREEN}$TestesFeitos${NADA}"
echo

echo -e "${NADA}${DESTACAR}1 - PROCURAR NOVOS TOKENS ${CINZA}MELHOR ESPERAR UM POUCO...${NADA}"
echo -e "${DESTACAR}2 - REPETIR OS TOKENS USADOS ${CINZA}RECOMENDADO 1X${NADA}"
echo -e "${DESTACAR}0 - VOLTAR${NADA}" 
echo
read -p "ESCOLHA UMA OPÇÃO: " denovo1
case $denovo1 in
1)rm -f TestesFeitos.txt ; Buscartokens;;
2)Testador1;;
0)MenuPrincipal;;
*)clear  ; Ler12;;
esac
}





