echo -e "${DESTACAR}INICIANDO A TESTAGEM DOS TOKENS ENCONTRADOS...\nNÃO DESCONECTE DA INTERNET ATÉ ACABAR${DESTACAR}"
sleep 4
Testador1
}




Testador1(){
termux-wake-lock
clear
echo
AZULALL='\e[01;37;44m'
VermelhoAll='\e[01;37;41m'
echo -e "${VermelhoAll}       NUMERO: $NumeroDeTelefone        ${NADA}"
echo -e "${VermelhoAll}         TESTAGEM INICIADA          ${NADA}"
echo -e "${AZULALL}====================================${NADA}"
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
echo -e "${LETRAPRETA}             TESTADOR 1             ${NADA}"
echo -e "${AZULALL}====================================${NADA}"

TesterElegibility=$(echo $impressionid1 | grep -n - | grep -c 1:)
case $TesterElegibility in
1) ;;
*)echo -e "${RED}NÃO ELEGIVEL NESSE TESTADOR${NADA}"; sleep 3; Testador2;;
esac

for i in $(seq 25);
do
reward1=$(curl -s "http://e.vivo.ddivulga.com/api/clickEvent" -d "advId=$advid1&eventImpressionId=$impressionid1&apass=$AccessPass")
rw1=$(echo $reward1 | grep -c "evtClickId")
case $rw1 in
1) echo -e "${GREEN}PROVAVEL ENVIO DOS MBS - $i/25${NADA}" ; sleep 3;;
0)echo -e "${RED}NÃO ELEGIVEL NESSE TESTADOR${NADA}"; sleep 3; Testador2;;
esac
done
Testador2
}






